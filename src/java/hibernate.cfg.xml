<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/onps?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="com/hp/pojo/Electionresult.hbm.xml"/>
    <mapping resource="com/hp/pojo/City.hbm.xml"/>
    <mapping resource="com/hp/pojo/Candidate.hbm.xml"/>
    <mapping resource="com/hp/pojo/Users.hbm.xml"/>
    <mapping resource="com/hp/pojo/Electiondetails.hbm.xml"/>
    <mapping resource="com/hp/pojo/Candidatepromises.hbm.xml"/>
    <mapping resource="com/hp/pojo/Voter.hbm.xml"/>
    <mapping resource="com/hp/pojo/Politicalparty.hbm.xml"/>
    <mapping resource="com/hp/pojo/Questionanswer.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
